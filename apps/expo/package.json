    "react-native-web": "~0.19.12"
  },
  "devDependencies": {
    "@babel/core": "^7.24.6",
    "@expo/metro-config": "~0.18.4",
    "@tamagui/babel-plugin": "^1.101.0",
    "metro-minify-terser": "^0.80.9",
    "typescript": "^5.4.5"
  }
