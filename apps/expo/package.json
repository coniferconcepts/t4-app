{
  "name": "expo-app",
  "version": "2.0.0",
  "main": "index.js",
  "private": true,
  "scripts": {
    "ios": "expo run:ios",
    "android": "expo run:android",
    "dev": "expo start --dev-client",
    "dev:prod": "expo start --no-dev --minify",
    "prebuild": "expo prebuild",
    "build:ios:preview": "eas build --platform ios --profile preview --local",
    "build:android:preview": "eas build --platform android --profile preview --local",
    "build:ios": "eas build --platform ios --profile production --local",
    "build:android": "eas build --platform android --profile production --local",
    "clean": "git clean -xdf .expo node_modules ios android"
  },
  "dependencies": {
    "@babel/runtime": "^7.24.6",
    "@shopify/flash-list": "^1.6.3",
    "@t4/ui": "workspace:*",
    "app": "workspace:*",
    "babel-plugin-module-resolver": "^5.0.2",
    "burnt": "^0.12.2",
    "expo": "^51.0.8",
    "expo-apple-authentication": "~6.4.1",
    "expo-constants": "~16.0.1",
    "expo-crypto": "~13.0.2",
    "expo-dev-client": "~4.0.14",
    "expo-font": "~12.0.5",
    "expo-image": "~1.12.9",
    "expo-linear-gradient": "~13.0.2",
    "expo-linking": "6.3.1",
    "expo-router": "^3.5.14",
    "expo-splash-screen": "^0.27.4",
    "expo-status-bar": "~1.12.1",
    "expo-system-ui": "^3.0.4",
    "expo-updates": "^0.25.14",
    "expo-web-browser": "~13.0.3",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-native": "^0.74.1",
    "react-native-gesture-handler": "~2.16.2",
    "react-native-mmkv": "^2.12.2",
    "react-native-safe-area-context": "4.10.1",
    "react-native-screens": "^3.31.1",
    "react-native-svg": "^15.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.24.6",
    "@expo/metro-config": "^0.18.4",
    "@tamagui/babel-plugin": "1.75.9",
    "metro-minify-terser": "^0.80.9",
    "typescript": "^5.4.5"
  }
}
